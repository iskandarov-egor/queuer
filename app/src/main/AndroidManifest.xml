<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.nbsp.queuer"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
        >
        </activity>
        <activity
            android:name=".ui.queue.admin.QueueAdminActivity"
            android:label="@string/title_activity_queue_admin"
        >
        </activity>
        <activity
            android:name=".ui.queue.member.QueueMemberActivity"
            android:label="hello"
        >
        </activity>
        <activity
            android:name=".ui.queues.MyQueuesActivity"
            android:label="Мои Очереди">
        </activity>
        <activity android:name=".ui.preference.PreferenceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
